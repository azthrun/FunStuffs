require 'rho'
require 'rho/rhocontroller'
require 'rho/rhoerror'
require 'helpers/browser_helper'

class SettingsController < Rho::RhoController
  include BrowserHelper
  
  def setup
    render
  end
  
  def wait
    mode = @params["mode"]
    render
  end

  def storePanelId
    panelId = @params["panelId"]
    Settings.set(Settings::PANELID, panelId)
    render :layout => false
  end
  
end
